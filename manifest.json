{
    "name": "Hello World!",
    "description": "first Extension!",
    
    "version": "1.0",
    "manifest_version": 3,
    "background": {
      "service_worker": "background.js"
    },
    "permissions":["storage","tabs","activeTab","storage"],

    "action":{
      "default_popup":"popup.html",
      "default_icon":{
        "16": "/images/tabHolder16.png",
        "32": "/images/tabHolder32.png",
        "48": "/images/tabHolder48.png",
        "128": "/images/tabHolder128.png" 
      }
    },

    "icons":{
      "16": "/images/tabHolder16.png",
      "32": "/images/tabHolder32.png",
      "48": "/images/tabHolder48.png",
      "128": "/images/tabHolder128.png" 
    },
    "options_page":"options.html"
}
