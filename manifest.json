{
    "name": "tabHolder",
    "description": "You can manage your tabs and save them.",
    
    "version": "2.0",
    "manifest_version": 3,
    "background": {
      "service_worker": "background.js",
      "type":"module"
    },

    "permissions":[
      "storage",
      "contextMenus",
      "tabs"
    ],
    
    "action":{
      "default_icon":{
        "16": "/images/tabHolder16.png",
        "32": "/images/tabHolder32.png"
      }
    },
    "icons": {
      "16": "/images/tabHolder16.png",
      "32": "/images/tabHolder32.png",
      "48": "/images/tabHolder48.png",
      "128": "/images/tabHolder128.png" 
    },

    "commands": {
      "open-app": {
        "suggested_key": {
          "default": "Ctrl+Shift+0",
          "mac": "Command+Shift+0"
        },
        "description": "Open tab holder app instantly"
      }
    }
}
